    "scripts": {
        "rimraf": "rimraf",
        "tslint": "tslint",
        "typedoc": "typedoc",
        "webpack": "webpack --progress --profile --bail",
        "webpack-dev-server": "webpack-dev-server",
        "webdriver-manager": "webdriver-manager",
        "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage compiled dll dist-admin dist-public dist-mobile",
        "clean:dist:admin": "npm run rimraf -- dist-admin",
        "clean:dist:public": "npm run rimraf -- dist-public",
        "clean:dist:mobile": "npm run rimraf -- dist-mobile",
        "clean:dll": "npm run rimraf -- dll",
        "clean:electron": "npm run rimraf -- build",
        "preclean:install": "npm run clean",
        "clean:install": "npm set progress=false && npm install",
        "preclean:start": "npm run clean",
        "clean:start": "npm start",
        "watch": "npm run watch:dev",
        "watch:dev": "npm run build:dev -- --watch",
        "watch:dev:hmr": "npm run watch:dev -- --hot",
        "watch:prod": "npm run build:prod -- --watch",
        "build": "npm run build:dev",
        "prebuild:dev": "npm run clean:dist:public",
        "build:dev": "npm run webpack-dev-server  --inline --progress -- --config config/webpack.dev.js",
        "prebuild:prod": "npm run clean:dist:public",
        "build:prod": "npm run webpack -- --config config/webpack.prod.js",
        "build:admin": "npm run build:admin:dev",
        "prebuild:admin:dev": "npm run clean:dist:admin",
        "build:admin:dev": "npm run clean:dist:admin && npm run webpack-dev-server  --inline --progress -- --config config/webpack.admin.dev.js",
        "prebuild:admin:prod": "npm run clean:dist:admin",
        "build:admin:prod": "npm run webpack -- --config config/webpack.admin.prod.js",
        "build:mobile": "npm run build:mobile:dev",
        "prebuild:mobile:dev": "npm run clean:dist:mobile",
        "build:mobile:dev": "npm run clean:dist:mobile && npm run webpack-dev-server  --inline --progress -- --config config/webpack.mob.dev.js",
        "prebuild:mobile:prod": "npm run clean:dist:mobile",
        "build:mobile:prod": "npm run webpack -- --config config/webpack.mob.prod.js",
        "server": "npm run server:dev",
        "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base public/",
        "server:dev:hmr": "npm run server:dev -- --inline --hot",
        "server:prod": "http-server dist-public --cors",
        "server:admin": "npm run server:admin:dev",
        "server:admin:dev": "webpack-dev-server --config config/webpack.admin.dev.js --progress --profile --watch --content-base src/",
        "server:admin:dev:hmr": "npm run server:admin:dev -- --inline --hot",
        "server:admin:prod": "http-server dist-admin --cors",
        "server:mobile": "npm run server:mobile:dev",
        "server:mobile:dev": "webpack-dev-server --config config/webpack.mob.dev.js --progress --profile --watch --content-base src/",
        "server:mobile:dev:hmr": "npm run server:mobile:dev -- --inline --hot",
        "server:mobile:prod": "http-server dist-mobile --cors",
        "webdriver:update": "npm run webdriver-manager update",
        "webdriver:start": "npm run webdriver-manager start",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "ngc": "./node_modules/.bin/ngc-w -p tsconfig.admin.aot.json",
        "pree2e": "npm run webdriver:update -- --standalone",
        "pretest": "npm run lint",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "gh-pages": "wintersmith build -C docs && gh-pages -d docs/build",
        "start": "npm run build:admin",
        "start:hmr": "npm run server:dev:hmr",
        "version": "npm run build",
        "postversion": "git push && git push --tags",
        "build:electron": "npm run build:electron.full",
        "prebuild:electron.full": "npm run clean:electron",
        "build:electron.full": "npm run build:electron.renderer && npm run build:electron.main",
        "postbuild:electron.full": "npm run electron:start",
        "build:electron.renderer": "npm run webpack -- --config config/electron/webpack.renderer.prod.js",
        "build:electron.main": "npm run webpack -- --config config/electron/webpack.electron.prod.js",
        "electron:start": "electron build",
        "build:admin:prod:aot": "npm run clean:dist:admin && npm run clean:admin:aot && webpack --config config/webpack.admin.prod.js  --progress --profile --bail",
        "build:mobile:prod:aot": "npm run clean:dist:mobile &&  npm run clean:mobile:aot && webpack --config config/webpack.mob.prod.js  --progress --profile --bail",
        "build:admin:aot": "npm run build:admin:prod:aot",
        "build:mobile:aot": "npm run build:mobile:prod:aot",
        "clean:admin:aot": "npm run rimraf -- compiled",
        "clean:mobile:aot": "npm run rimraf -- compiled",
        "build:prod:aot": "npm run clean:dist:public && npm run clean:aot && webpack --config config/webpack.prod.js  --progress --profile --bail",
        "build:aot": "npm run build:prod:aot",
        "clean:aot": "npm run rimraf -- compiled"
    },
